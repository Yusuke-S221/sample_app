<div class="new-post-space">
  <%= form_tag("/posts/create", {multipart: true}) do %>
    <div class="new-post-form">
      <p>photo</p>
      <input name="photo" type="file">
      <p>title</p>
      <input name="title">
      <p>content</p>
      <input name="content">
      <p>post</p>
      <input type="submit" value="submit">
    </div>
  <% end %>
</div>
